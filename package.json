{
  "version": "1.21.4-alpha",
  "name": "@crisvp/pocketbase-js",
  "description": "PocketBase JavaScript SDK",
  "author": "H.C. van Pelt",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/crisvp/pocketbase-js.git"
  },
  "exports": {
    ".": {
      "import": "./dist/pocketbase-js.mjs",
      "require": "./dist/pocketbase-js.js",
      "types": "./dist/pocketbase-js.d.ts"
    },
    "./src": {
      "require": "./build/index.js",
      "types": "./dist/pocketbase-js.d.ts"
    }
  },
  "module": "./dist/pocketbase-js.mjs",
  "types": "./dist/pocketbase-js.d.ts",
  "keywords": [
    "pocketbase",
    "pocketbase-js",
    "js-sdk",
    "javascript-sdk",
    "pocketbase-sdk"
  ],
  "scripts": {
    "build": "vite build",
    "coverage": "vitest run --coverage",
    "compile": "tsc -p tsconfig.json",
    "test": "vitest run",
    "lint": "pnpm compile ; eslint ."
  },
  "browserslist": [
    "defaults and fully supports es6-module",
    "maintained node versions",
    "last 2 versions",
    "not dead",
    "> 0.2%"
  ],
  "devDependencies": {
    "@crisvp/prettier-config": "workspace:^",
    "@eslint/js": "^9.4.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.14.2",
    "@vitest/coverage-v8": "^1.6.0",
    "browserslist-to-esbuild": "^2.1.1",
    "eslint": "^8.57.0",
    "happy-dom": "^14.12.3",
    "msw": "^2.3.1",
    "object-to-formdata": "^4.5.1",
    "prettier": "3.3.2",
    "ts-jest": "^29.1.5",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.0",
    "vite": "^5.2.12",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "jose": "^5.4.1"
  }
}
