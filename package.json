{
    "version": "1.21.3",
    "name": "@crisvp/pocketbase-js",
    "description": "PocketBase JavaScript SDK",
    "author": "H.C. van Pelt",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/crisvp/pocketbase-js.git"
    },
    "exports": {
        ".": {
            "import": "./dist/pocketbase-js.mjs",
            "require": "./dist/pocketbase-js.js",
            "types": "./dist/pocketbase-js.d.ts"
        }
    },
    "module": "./dist/pocketbase-js.mjs",
    "types": "./dist/pocketbase-js.d.ts",
    "keywords": [
        "pocketbase",
        "pocketbase-js",
        "js-sdk",
        "javascript-sdk",
        "pocketbase-sdk"
    ],
    "scripts": {
        "build": "vite build",
        "compile": "tsc -p .",
        "test": "vitest run"
    },
    "prettier": {
        "editorconfig": true,
        "tabWidth": 4,
        "printWidth": 85,
        "bracketSameLine": true
    },
    "browserslist": [
        "defaults and fully supports es6-module",
        "maintained node versions",
        "last 2 versions",
        "not dead",
        "> 0.2%"
    ],
    "devDependencies": {
        "@eslint/js": "^9.4.0",
        "@types/eslint__js": "^8.42.3",
        "@types/node": "^20.14.2",
        "browserslist-to-esbuild": "^2.1.1",
        "eslint": "^8.57.0",
        "prettier": "3.2.4",
        "typescript": "^5.4.5",
        "typescript-eslint": "^7.13.0",
        "vite": "^5.2.12",
        "vite-plugin-dts": "^3.9.1",
        "vitest": "^0.34.1"
    }
}
